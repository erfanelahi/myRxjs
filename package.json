{
  "name": "angular2-seed",
  "version": "1.0.0",
  "description": "A simple starter Angular2 project",
  "scripts": {
    "rimraf": "rimraf",
    "clean:dist": "npm run rimraf -- dist .awcache",
    "clean:compile": "npm run rimraf -- \"src/compiled\" ",
    "ngc": "./node_modules/.bin/ngc -p tsconfig.aot.json --locale=en-US && npm run rimraf -- output",
    "start-prod": "npm run clean:dist && npm run clean:compile && npm run ngc && webpack-dev-server --progress --config webpack.prod.config.js --port 5000 --open",
    "build-prod": "npm run clean:dist && npm run clean:compile && npm run ngc && webpack --progress --config webpack.prod.config.js",
    "server": "json-server --watch server/api/db.json",
    "start-dev": "concurrently \"npm run clean:dist && npm run clean:compile && webpack-dev-server --progress --watch --output-pathinfo --config webpack.dev.config.js\" \"npm run server\"",
    "build-dev": "npm run clean:dist && npm run clean:compile && webpack --progress --config webpack.dev.config.js"
  },
  "contributors": [
    "Rob Wormald <robwormald@gmail.com>",
    "PatrickJS <github@gdi2290.com>"
  ],
  "license": "MIT",
